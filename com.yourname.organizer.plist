<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.yourname.organizer</string>

    <key>ProgramArguments</key>
    <array>
        <string>"PATH_TO_PYTHON3"</string>
        
        <string>"PATH_TO_SCRIPT"</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/organizer.out</string>
    <key>StandardErrorPath</key>
    <string>/tmp/organizer.err</string>
</dict>
</plist>
