<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.yourname.organizer</string>

    <key>WorkingDirectory</key>
    <string>/PATH/TO/YOUR/PROJECT_FOLDER</string>

    <key>ProgramArguments</key>
    <array>
        <string>/PATH/TO/PYTHON</string>
        
        <string>organizer.py</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardErrorPath</key>
    <string>/tmp/organizer_service.err</string>
</dict>
</plist>