{
  "source_folder": "/PATH/TO/YOUR/DOWNLOADS",
  "rules": {
    "Images": {
      "target_path": "/PATH/TO/YOUR/PICTURES",
      "extensions": [
        ".jpg", ".jpeg", ".png", ".gif", ".svg", ".heic", ".bmp", ".tiff"
      ]
    },
    "Documents": {
      "target_path": "/PATH/TO/YOUR/DOCUMENTS/",
      "extensions": [
        ".pdf", ".docx", ".doc", ".txt", ".xlsx", ".pptx", ".csv", ".md"
      ]
    },
    "Archives": {
      "target_path": "/PATH/TO/YOUR/DOCUMENTS/Archives",
      "extensions": [
        ".zip", ".tar", ".gz", ".rar", ".7z"
      ]
    },
    "Installers": {
      "target_path": "/PATH/TO/YOUR/DOWNLOADS/Installers",
      "extensions": [
        ".dmg", ".pkg", ".exe", ".msi", ".iso"
      ]
    },
    "Scripts": {
      "target_path": "/PATH/TO/YOUR/DOCUMENTS/Scripts",
      "extensions": [
        ".py", ".sh", ".js", ".html", ".css"
      ]
    }
  }
}